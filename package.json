{
  "name": "fullstack-base",
  "version": "0.0.1",
  "description": "A fullstack base repo using Express.js, React.js, Vite, and TypeScript.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "format": "biome format . --write",
    "check": "npm-run-all check:lint check:format check:types",
    "check:lint": "biome check .",
    "check:format": "biome ci .",
    "check:types": "tsc --noEmit",
    "db:setup": "tsx server/database-setup.ts",
    "db:setup:prod": "node dist/server/database-setup.js",
    "start:web": "vite",
    "start:server": "tsx --watch server/index.ts",
    "start:production": "node dist/server/index.js",
    "build": "npm run build:web && npm run build:server",
    "build:web": "vite build",
    "build:server": "tsc -p server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "gl-matrix": "^3.4.3",
    "jsonwebtoken": "^9.0.2",
    "obj-file-parser": "^0.5.3",
    "pg": "8.8.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-modal-promise": "^1.0.2",
    "react-router-dom": "~6.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/obj-file-parser": "^0.5.0",
    "@types/pg": "8.6.5",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^3.0.0",
    "identity-obj-proxy": "3.0.0",
    "npm-run-all": "4.1.5",
    "supertest": "6.3.1",
    "tsx": "^4.7.0",
    "typescript": "4.8.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-sass-dts": "^1.3.9"
  }
}
