{
  "name": "glsl-playgroun",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "mocha --require tsx --exit  'server/**/*.test.ts' 'client/**/*.test.ts'",
    "format": "biome format . --write",
    "check": "npm-run-all check:lint check:format check:types",
    "check:lint": "biome check .",
    "check:format": "biome ci .",
    "check:types": "tsc --noEmit",
    "db:setup": "tsx server/database-setup.ts",
    "start:web": "vite",
    "start:server": "tsx --watch server/index.ts",
    "start:production": "node dist/server/index.js",
    "build": "npm run build:web && npm run build:server",
    "build:web": "vite build",
    "build:server": "tsc -p server"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "bcrypt": "5.1.1",
    "chai": "^5.0.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "gl-matrix": "^3.4.3",
    "jsonwebtoken": "^9.0.2",
    "obj-file-parser": "^0.5.3",
    "pg": "8.8.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-modal-promise": "^1.0.2",
    "react-router-dom": "~6.3.0"
