{
  "name": "fullstack-base",
  "version": "0.0.1",
  "description": "A fullstack base repo using Express.js, React.js, Webpack, and TypeScript.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint --ext js --ext jsx client server *.js",
    "setup-db": "ts-node --esm setup-db.ts",
    "start": "npm-run-all --parallel start:server start:web",
    "start:production": "node dist/server/index.js",
    "start:server": "nodemon --esm server/index.ts",
    "start:web": "webpack serve --hot --mode development --devtool eval-source-map",
    "start:web:watch": "npm run start",
    "build:web": "webpack --devtool source-map",
    "build:server": "tsc -p server",
    "build": "npm-run-all --parallel build:web build:server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
